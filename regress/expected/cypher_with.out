-- Chained WITH tests
--
SELECT * FROM cypher('with_accessor_opt', $$
    EXPLAIN (VERBOSE, COSTS OFF)
    MATCH (a:Person)-[r:KNOWS]->(b:Person)
    WITH a, b, id(a) AS aid
    WITH a.name AS aname, b.name AS bname, aid
    RETURN aname, bname, aid
$$) AS (aname agtype, bname agtype, aid agtype);
                                                                                QUERY PLAN                                                                                
--------------------------------------------------------------------------------------------------------------------------------------------------------------------------
 Hash Join
   Output: agtype_access_operator(VARIADIC ARRAY[a.properties, '"name"'::agtype]), agtype_access_operator(VARIADIC ARRAY[b.properties, '"name"'::agtype]), (a.id)::agtype
   Inner Unique: true
   Hash Cond: (r.end_id = b.id)
   ->  Hash Join
         Output: a.properties, a.id, r.end_id
         Inner Unique: true
         Hash Cond: (r.start_id = a.id)
         ->  Seq Scan on with_accessor_opt."KNOWS" r
               Output: r.id, r.start_id, r.end_id, r.properties
         ->  Hash
               Output: a.properties, a.id
               ->  Seq Scan on with_accessor_opt."Person" a
                     Output: a.properties, a.id
   ->  Hash
         Output: b.properties, b.id
         ->  Seq Scan on with_accessor_opt."Person" b
               Output: b.properties, b.id
